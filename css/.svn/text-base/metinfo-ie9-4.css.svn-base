/* Icons */
@font-face
{
    font-family: 'slick';
    font-weight: normal;
    font-style: normal;

    src: url(../../../templates/M1156008/min/widget/slick/./fonts/slick.eot);
    src: url(../../../templates/M1156008/min/widget/slick/./fonts/slick.eot?#iefix) format('embedded-opentype'), url(../../../templates/M1156008/min/widget/slick/./fonts/slick.woff) format('woff'), url(../../../templates/M1156008/min/widget/slick/./fonts/slick.ttf) format('truetype'), url(../../../templates/M1156008/min/widget/slick/./fonts/slick.svg#slick) format('svg');
